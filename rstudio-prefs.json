{
  "console_double_click_select": true,
  "help_font_size_points": 12,
  "full_project_path_in_window_title": true,
  "save_workspace": "never",
  "load_workspace": false,
  "remove_history_duplicates": true,
  "restore_last_project": true,
  "posix_terminal_shell": "bash",
  "soft_wrap_r_files": true,
  "continue_comments_on_newline": true,
  "margin_column": 72,
  "syntax_color_console": true,
  "highlight_r_function_calls": true,
  "auto_append_newline": true,
  "strip_trailing_whitespace": true,
  "check_arguments_to_r_function_calls": true,
  "check_unexpected_assignment_in_function_call": true,
  "warn_if_no_such_variable_in_scope": true,
  "style_diagnostics": true,
  "default_encoding": "UTF-8",
  "save_files_before_build": true,
  "show_rmd_render_command": true,
  "default_sweave_engine": "knitr",
  "latex_shell_escape": true,
  "show_publish_diagnostics": true,
  "editor_theme": "Pastel On Dark",
  "panes": {
    "quadrants": [
      "Source",
      "Console",
      "TabSet1",
      "TabSet2"
    ],
    "tabSet1": [
      "Environment",
      "History",
      "Connections",
      "Build",
      "VCS",
      "Tutorial",
      "Presentation"
    ],
    "tabSet2": [
      "Files",
      "Plots",
      "Packages",
      "Help",
      "Viewer"
    ],
    "hiddenTabSet": [],
    "console_left_on_top": false,
    "console_right_on_top": true,
    "additional_source_columns": 1
  },
  "jobs_tab_visibility": "shown",
  "shiny_background_jobs": true,
  "shiny_viewer_type": "pane",
  "default_project_location": "/",
  "auto_expand_error_tracebacks": true,
  "initial_working_directory": "/project",
  "document_author": "UBEP",
  "rmd_viewer_type": "pane",
  "cran_mirror": {
    "name": "Custom",
    "host": "Custom",
    "url": "https://packagemanager.posit.co/cran/__linux__/jammy/2024-01-09",
    "secondary": "",
    "country": "",
    "changed": false
  },
  "use_tinytex": true,
  "pdf_previewer": "none"
}
